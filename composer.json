{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/api-pack": "1.1.0",
        "indragunawan/api-rate-limit-bundle": "0.2.1",
        "lexik/jwt-authentication-bundle": "2.6.1",
        "sensio/framework-extra-bundle": "5.2.4",
        "stof/doctrine-extensions-bundle": "1.3.0",
        "symfony/asset": "3.4.20",
        "symfony/console": "3.4.20",
        "symfony/dotenv": "3.4.20",
        "symfony/expression-language": "3.4.20",
        "symfony/flex": "1.1.8",
        "symfony/form": "3.4.20",
        "symfony/framework-bundle": "3.4.20",
        "symfony/monolog-bundle": "3.3.1",
        "symfony/orm-pack": "1.0.5",
        "symfony/process": "3.4.20",
        "symfony/security-bundle": "3.4.20",
        "symfony/serializer-pack": "1.0.2",
        "symfony/swiftmailer-bundle": "3.2.4",
        "symfony/translation": "3.4.20",
        "symfony/twig-bundle": "3.4.20",
        "symfony/validator": "3.4.20",
        "symfony/web-link": "3.4.20",
        "symfony/webpack-encore-bundle": "1.0.0",
        "symfony/yaml": "3.4.20",
        "twig/extensions": "1.5.4"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "3.1.0",
        "symfony/debug-pack": "1.0.7",
        "symfony/maker-bundle": "1.11.1",
        "symfony/profiler-pack": "1.0.4",
        "symfony/test-pack": "1.0.2",
        "symfony/web-server-bundle": "3.4.20"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.0.17",
        "symfony/polyfill-ctype": "1.10.0",
        "symfony/polyfill-iconv": "1.10.0",
        "symfony/polyfill-php70": "1.10.0",
        "symfony/polyfill-php71": "1.10.0",
        "symfony/polyfill-php56": "1.10.0"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "3.4.20"
        }
    }
}
